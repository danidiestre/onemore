EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Optional: custom domain for invite links (defaults to app.json extra.inviteLinkDomain or drinkcounter://)
# EXPO_PUBLIC_INVITE_LINK_DOMAIN=https://links.yourapp.com

